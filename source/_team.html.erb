<div class="text-center">
  <h1> Our Team </h1>
</div>

<ul class="list-inline text-center">
  <% ["boris", "seb", "romain"].each do |member| %>
    <li>
      <%= image_tag "#{member}.png", class: "img-circle" %>
      <h3><%= member.capitalize %></h3>
    </li>
  <% end %>
</ul>

<div class="text-center padded contact-makeup">
  <% link_to "contact@airbnb.com" do %>
  <i class="fa fa-envelope"> contact@airbnb.com</i>
  <% end %>
</div>
